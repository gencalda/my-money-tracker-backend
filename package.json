{
  "name": "my-money-tracker-backend",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup": "npm install && sls dynamodb install --verbose",
    "dynamodb:start": "sls dynamodb start",
    "start": "set AWS_STAGE=local && sls offline start",
    "start:dev": "set AWS_STAGE=dev && sls offline start",
    "start:prod": "set AWS_STAGE=production && sls offline start",
    "format": "npx prettier --write src/ --verbose",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "deploy:dev": "set AWS_STAGE=dev && serverless deploy"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.53.0",
    "@aws-sdk/util-dynamodb": "^3.53.0",
    "@middy/core": "^2.5.3",
    "@middy/http-json-body-parser": "^2.5.3",
    "aws-lambda": "^1.0.7",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "jwt-decode": "^3.1.2",
    "multer": "^1.4.4",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-http": "^2.7.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/express": "^4.17.13",
    "@types/multer": "^1.4.7",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.1",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "json-schema-to-ts": "^1.5.0",
    "prettier": "^2.5.1",
    "serverless": "^3.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^8.5.0",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
